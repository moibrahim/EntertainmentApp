<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

    <Button
        android:id="@+id/btnComments"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/btnMessages"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:layout_alignTop="@+id/tvmsgs"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="55dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:layout_marginBottom="58dp"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <TextView
        android:id="@+id/tvComments"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="141dp"
        android:layout_toEndOf="@+id/imageView6"
        android:gravity="center|left"
        android:text="Comments"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="55dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:layout_marginBottom="201dp"
        app:srcCompat="@android:drawable/ic_menu_save" />

    <TextView
        android:id="@+id/tvmsgs"
        android:layout_width="116dp"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvSavedpost"
        android:layout_marginBottom="70dp"
        android:gravity="center|left"
        android:text="Meassages"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="55dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:layout_marginBottom="130dp"
        app:srcCompat="@android:drawable/ic_menu_add" />

    <TextView
        android:id="@+id/tvSavedpost"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="209dp"
        android:layout_toEndOf="@+id/imageView6"
        android:gravity="center|left"
        android:text="Saved Posts"
        android:textSize="18sp" />

    <View
        android:id="@+id/divider5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="158dp"
        android:background="?android:attr/listDivider" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="130dp"
        android:layout_height="122dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="17dp"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/tvUsernameProfile"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="66dp"
        android:gravity="center|left"
        android:text="@string/tvUsernameProfile"
        android:textSize="30sp"
        android:textStyle="bold" />

    <RatingBar
        android:id="@+id/ratingBar"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="171dp"
        android:gravity="center"
        android:isIndicator="true"
        android:max="5" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="239dp"
        android:background="?android:attr/listDivider" />

    <Button
        android:id="@+id/btnSavedpost"
        android:layout_width="174dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tvSavedpost"
        android:layout_alignStart="@+id/tvUsernameProfile"
        android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/btnSettings"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView9"
        android:layout_marginEnd="30dp"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <ImageButton
        android:id="@+id/btnEditAcc"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_alignBottom="@+id/tvmsgs"
        android:layout_alignStart="@+id/btnSettings"
        app:srcCompat="@android:drawable/ic_menu_edit" />
    <android.support.design.widget.BottomNavigationView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        app:menu="@menu/menu_bottom_navigation" />

</RelativeLayout>